# Environment Configuration Example
# Copy this file to .env and update with your actual values

# Application Configuration
REACT_APP_NAME=Resume Generator
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=A modern web application for generating professional resumes

# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_VERSION=v1

# GitHub Integration (for deployment/CI)
# GITHUB_TOKEN=your_github_token_here

# Security
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_SENTRY_DSN=

# Development
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=error

# Feature Flags
REACT_APP_ENABLE_TEMPLATES=true
REACT_APP_ENABLE_EXPORT_PDF=true
REACT_APP_ENABLE_EXPORT_DOCX=true
REACT_APP_ENABLE_SHARING=true